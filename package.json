{
  "name": "gql-multipart",
  "description": "Complementary middleware for express-graphql and koa-graphql which adds support parsing multipart request and file uploads",
  "version": "0.0.10",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Arhun Hınçalan <ahincalan@hotmail.com>"
  ],
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqlorg/gql-multipart.git"
  },
  "keywords": [
    "javascript",
    "graphql",
    "multipart",
    "server",
    "middle-ware",
    "middleware",
    "upload",
    "file",
    "express-graphql",
    "koa-graphql"
  ],
  "dependencies": {
    "busboy": "^0.3.0",
    "http-errors": "^1.7.2",
    "tmp": "0.0.33",
    "ts-node": "^8.0.2"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/form-data": "^2.2.1",
    "@types/graphql": "^14.0.5",
    "@types/http-errors": "^1.6.1",
    "@types/koa": "^2.0.48",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.39",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.1.6",
    "@types/tmp": "^0.0.34",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "form-data": "^2.3.3",
    "graphql": "^14.1.1",
    "koa": "^2.7.0",
    "koa-graphql": "^0.8.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "mocha": "^6.0.0",
    "node-fetch": "^2.3.0",
    "nyc": "^13.3.0",
    "rejected-or-not": "^1.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">= 6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  },
  "scripts": {
    "test": "mocha -r ./test/support/env -r ts-node/register --reporter spec --bail --check-leaks ./test/*.ts",
    "cover": "nyc --reporter html --reporter text -r lcov npm run test",
    "travis-cover": "nyc --reporter lcovonly -r lcov npm run test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc"
  }
}

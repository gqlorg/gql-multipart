{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.5%, not dead, node 6"
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "@babel/plugin-transform-modules-commonjs",
      {
        "noInterop": true,
        "strict": true,
        "loose": true
      }
    ]
  ]
}
